<% include ./partials/header %>
<% blogs.forEach(blog=>{ %>
    <div class='blog'>
        <h1>
            <%= blog.title%>
        </h1>
        <img src= <%=blog.image %> width="250px" height="250px" alt="bird">
        <p>
            <%= blog.body.substring(0,50)%>...
        </p>
        <a href='/blogs/<%= blog._id%>'>More</a>
    </div>
    <% })%>
<% include ./partials/footer %>